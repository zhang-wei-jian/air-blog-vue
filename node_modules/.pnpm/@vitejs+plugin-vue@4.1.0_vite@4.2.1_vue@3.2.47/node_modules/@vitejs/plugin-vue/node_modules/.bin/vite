#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/vite@4.2.1_less@4.1.3/node_modules/vite/bin/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/vite@4.2.1_less@4.1.3/node_modules/vite/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/vite@4.2.1_less@4.1.3/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/vite@4.2.1_less@4.1.3/node_modules/vite/bin/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/vite@4.2.1_less@4.1.3/node_modules/vite/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/vite@4.2.1_less@4.1.3/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@4.2.1_less@4.1.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@4.2.1_less@4.1.3/node_modules/vite/bin/vite.js" "$@"
fi
