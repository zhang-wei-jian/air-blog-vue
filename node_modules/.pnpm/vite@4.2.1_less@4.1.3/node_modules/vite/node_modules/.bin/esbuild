#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/esbuild@0.17.15/node_modules/esbuild/bin/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/esbuild@0.17.15/node_modules/esbuild/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/esbuild@0.17.15/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/esbuild@0.17.15/node_modules/esbuild/bin/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/esbuild@0.17.15/node_modules/esbuild/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/esbuild@0.17.15/node_modules:/Users/zwj/Desktop/项目测试/测试跨域项目完成/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../../esbuild@0.17.15/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
